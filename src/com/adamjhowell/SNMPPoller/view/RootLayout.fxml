<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx = "http://javafx.com/fxml/1" alignment = "center" hgap = "10" vgap = "10"
          xmlns = "http://javafx.com/javafx/8.0.65" fx:controller = "com.adamjhowell.SNMPPoller.Controller">
	<rowConstraints>
		<RowConstraints minHeight = "10.0" prefHeight = "30.0"/>
		<RowConstraints minHeight = "10.0" prefHeight = "30.0"/>
		<RowConstraints minHeight = "10.0" prefHeight = "30.0"/>
		<RowConstraints minHeight = "10.0" prefHeight = "30.0"/>
		<RowConstraints minHeight = "10.0" prefHeight = "30.0"/>
	</rowConstraints>
	<columnConstraints>
		<ColumnConstraints minWidth = "30.0" prefWidth = "30.0"/>
		<ColumnConstraints minWidth = "200.0" prefWidth = "300.0"/>
		<ColumnConstraints minWidth = "200.0" prefWidth = "300.0"/>
		<ColumnConstraints minWidth = "30.0" prefWidth = "30.0"/>
	</columnConstraints>
	<children>
		<Label text = "Target" GridPane.columnIndex = "1" GridPane.rowIndex = "1"/>
		<Label text = "Community String" GridPane.columnIndex = "1" GridPane.rowIndex = "2"/>
		<TextField promptText = "IP or Hostname" text = "127.0.0.1" GridPane.columnIndex = "2"
		           GridPane.rowIndex = "1"/>
		<TextField GridPane.columnIndex = "2" GridPane.rowIndex = "2"/>
		<Button defaultButton = "true" mnemonicParsing = "false" text = "Scan" textAlignment = "CENTER"
		        GridPane.columnIndex = "1" GridPane.rowIndex = "3"/>
	</children>
</GridPane>
